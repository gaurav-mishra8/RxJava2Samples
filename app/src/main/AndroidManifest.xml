<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="greenbotsite.rxjava2samples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MapExampleActivity" />
        <activity android:name=".FlatMapExampleActivity" />
        <activity android:name=".ZipExampleActivity" />
        <activity android:name=".ThreadingExampleActivity" />
        <activity android:name=".AmbExampleActivity" />
        <activity android:name=".RepeatExampleActivity" />
        <activity android:name=".BufferExampleActivity" />
        <activity android:name=".RetryExampleActivity" />
        <activity android:name=".FilterExampleActivity" />
        <activity android:name=".SkipExampleActivity" />
        <activity android:name=".DebounceExampleActivity" />
        <activity android:name=".ScanExampleActivity" />
        <activity android:name=".TakeExampleActivity" />
    </application>

</manifest>